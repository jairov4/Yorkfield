<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>
<h1>Log Monitor</h1>

<table>
<tr>
	<td>Timestamp</td>
	<td>Severity</td>
	<td>Message</td>
</tr>
@Each.Items
<tr>
	<td>@Current.Timestamp</td>
	<td>@Current.Severity</td>
	<td>@Current.Message</td>
</tr>
@EndEach
</table>

</body>
</html>